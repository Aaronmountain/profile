{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es2017"
    ],
    "jsx": "preserve",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "incremental": true,
    "typeRoots": [
      "interfaces",
      "node_modules/@types"
    ],
    "baseUrl": "src",
    "paths": {
      "containers": [
        "containers"
      ],
      "components": [
        "components"
      ],
      "utils": [
        "utils"
      ],
      "constants": [
        "constants"
      ],
      "services": [
        "services"
      ],
      "hooks": [
        "hooks"
      ],
      "styles": [
        "styles"
      ],
      "statics": [
        "statics"
      ],
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}